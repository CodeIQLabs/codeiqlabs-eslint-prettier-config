{
  "name": "@codeiqlabs/eslint-prettier-config",
  "version": "1.8.0",
  "description": "Shared ESLint v9 flat config & Prettier config for CodeIQLabs projects",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./minimal": {
      "types": "./dist/minimal.d.ts",
      "import": "./dist/esm/minimal.js",
      "require": "./dist/cjs/minimal.js"
    },
    "./standard": {
      "types": "./dist/standard.d.ts",
      "import": "./dist/esm/standard.js",
      "require": "./dist/cjs/standard.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/esm/react.js",
      "require": "./dist/cjs/react.js"
    },
    "./prettier": "./dist/cjs/prettier-config.js",
    "./ignores": {
      "types": "./dist/ignores.d.ts",
      "import": "./dist/esm/ignores.js",
      "require": "./dist/cjs/ignores.js"
    },
    "./pre-commit": {
      "types": "./dist/pre-commit-config.d.ts",
      "import": "./dist/esm/pre-commit-config.js",
      "require": "./dist/cjs/pre-commit-config.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "tag": "latest",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "postinstall": "node -e \"try { require('child_process').execSync('husky', {stdio: 'inherit'}) } catch (e) { console.log('Husky setup skipped') }\"",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:status": "changeset status",
    "changeset:check": "find .changeset -name '*.md' ! -name 'README.md' ! -name 'RELEASE_TEMPLATE.md' | wc -l",
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs:pkg": "node -e \"require('fs').mkdirSync('dist/cjs',{recursive:true});require('fs').writeFileSync('dist/cjs/package.json','{\\\"type\\\":\\\"commonjs\\\"}\\n')\"",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:cjs:pkg && npm run format && npm run lint",
    "build:cjs:show": "tsc -p tsconfig.cjs.json --showConfig",
    "test:load": "node tests/load-config.test.cjs",
    "test:pre-commit": "node tests/pre-commit-config.test.cjs",
    "test:esm": "node tests/esm-import-test.mjs",
    "test:all": "npm run test:load && npm run test:pre-commit && npm run test:esm",
    "prepublishOnly": "npm run build && npm run test:load && npm run test:pre-commit && npm run test:esm",
    "release": "changeset publish"
  },
  "keywords": [
    "eslint",
    "prettier",
    "config",
    "flat-config",
    "codeiqlabs"
  ],
  "author": "CodeIQLabs",
  "license": "MIT",
  "engines": {
    "node": ">=18.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeIQLabs/codeiqlabs-eslint-prettier-config"
  },
  "bugs": {
    "url": "https://github.com/CodeIQLabs/codeiqlabs-eslint-prettier-config/issues"
  },
  "homepage": "https://github.com/CodeIQLabs/codeiqlabs-eslint-prettier-config#readme",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.0.0",
    "prettier": "^3.3.3",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/eslint": "^9.6.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.4.0"
  }
}
